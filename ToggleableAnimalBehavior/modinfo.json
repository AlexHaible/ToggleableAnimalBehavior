{
  "name": "Toggleable Animal Behavior",
  "author": "Sauravisus",
  "description": "Allows the users to set the behavior of all animals individually, as well as how often the shark attacks the raft, and if the seagulls attack crops.",
  "version": "1.0.4",
  "license": "GNU AGPLv3",
  "icon": "icon.png",
  "banner": "banner.png",
  "gameVersion": "1.0",
  "requiredByAllPlayers": true,
  "updateUrl": "https://www.raftmodding.com/api/v1/mods/toggleable-animal-behavior/version.txt",
  "isModPermanent": false,
  "excludedFiles": [],
  "modSettings": [
    {
      "type": "checkbox",
      "name": "sharkIsTame",
      "text": "Is shark tame?",
      "default": false,
      "access": "both"
    },
    {
      "type": "input",
      "name": "sharkAtkMulti",
      "text": "Shark attack frequency (0 for none)",
      "default": "1.0",
      "mode": "DecimalNumber",
      "access": "both"
    },
    {
      "type": "checkbox",
